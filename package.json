    "jquery": "^3.2.1",
    "jquery-confirm": "^3.3.2",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "redux-token-auth": "^0.19.0",
    "toastr": "2.1.2"
