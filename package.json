{
  "name": "shoppinglist-frontend",
  "version": "1.0.0",
  "description": "A React.js application that consumes the shoppinglist api and dispalys an interactive user interface",
  "scripts": {
    "start": "babel-node configurations/serverConfig.js",
    "lint": "node_modules/.bin/esw webpack.config.* src configurations",
    "clean-dist": "node_modules/.bin/rimraf ./dist && mkdir dist",
    "build-html": "babel-node configurations/buildIndex.html.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node configurations/build.js",
    "postbuild": "babel-node configurations/serverConfig.prod.js"
    "test": "jest src --coverage",
    "coveralls": "npm run test && cat ./coverage/lcov.info | coveralls"
  },
